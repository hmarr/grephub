{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "api/main.go",
      "use": "@now/go"
    }
  ],
  "routes": [
    { "src": "^/api/.*", "dest": "/api/main.go" },
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
